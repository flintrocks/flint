external trait Test {
  func echo(message: string) -> string
}

contract Caller {  }

Caller :: (any) {
  public init() {}

  public func callThem(message: String) -> String {
    let test: Test = Test(address: 0x692a70d2e424a56d2c6c27aa97d1a86395877b3a)
    let theMessage: String = (call! test.echo(message: "Hello World" as! string)) as! String
    return theMessage
  }
}
