external trait Test {
  func echo(message: int256) -> int256
}

contract Caller {
  event DankTest(message: Int)
}

Caller :: (any) {
  public init() {}

  public func callThem() -> Int {
    let test: Test = Test(address: 0x5c3c1540dfcd795b0aca58a496e3c30fe2405b07)
    let theMessage: Int = (call! test.echo(message: 69 as! int256)) as! Int

    emit DankTest(message: theMessage)

    return theMessage
  }
}
