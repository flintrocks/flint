contract XCall {
  var contractToCall: Address
}

external trait ExternalContract {
  func emitCheck(value: uint8) -> uint8 
}

XCall :: caller <- (any) {
  public init(contractToCall: Address) {
    self.contractToCall = contractToCall
  }

  public func callExternal() {
    let externalContract: ExternalContract = ExternalContract(addresss: contractToCall) 
    let ret: Int = (call! externalContract.emitCheck(value: 0 as! uint8)) as! Int
  } 
}

