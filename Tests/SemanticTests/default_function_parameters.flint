// RUN: %flintc %s --verify

contract Test {
  var a: Int = 0
  var b: Int = 1
}

Test :: (any) {
  public init() {}
}

Test :: caller <- (any) {
  mutating func test1(a: Int = 0, b: Int = 1) {
    self.a = a;
    self.b = b;
  }

  mutating func test2() {
    test1();
    test1(a:2);
    test1(b:5);
    test1(a:3, b:2);
  }
}
